<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#ffffff" />
            <stroke
                android:width="4dp"
                android:color="#33B5E5"
                android:dashGap="0dp"
                android:dashWidth="6dp" />
            <corners
                android:radius="0dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#ffffff" />
            <stroke
                android:width="2dp"
                android:color="#33B5E5"
                android:dashGap="4dp"
                android:dashWidth="6dp" />
            <corners
                android:radius="0dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>